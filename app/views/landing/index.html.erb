<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <div id="new-user">
    <%= link_to "Create User", register_path, method: :get, local: true %>
  </div>
  <%= link_to "I already have an account", login_path %>
<% end %>

<div id="existing-users">
  <h2>Existing Users</h2>
  <% @users.each do |user| %>
    <ul>
      <li><%= link_to user.email, user_path(user) %></li>
    </ul>
  <% end %>
</div>